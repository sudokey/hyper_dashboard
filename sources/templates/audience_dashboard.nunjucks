            <div class="panel panel_app">
              <div class="app-info">
                <div class="app-info__icon">
                  <span class="icon icon_apple"></span>
                </div>
                <div class="app-info__img">
                  <img class="pick pick_app" src="img/slack.png">
                </div>
                <div class="app-info__name">
                  <a href="monetize_app_settings.html">Slack — Be less busy</a>
                </div>
                <div class="app-info__settings">
                  <a class="icon icon_settings" href="#"></a>
                </div>
              </div>

              <div class="inline inline_table">
                <div class="inline__item">
                  <div class="indicator">
                    <div class="indicator__value indicator__value_small">10.2M</div>
                    <div class="indicator__name indicator__name_light">MAU</div>
                  </div>
                </div>
                <div class="inline__item">
                  <div class="indicator">
                    <div class="indicator__value indicator__value_small">6M</div>
                    <div class="indicator__name indicator__name_light">DAU</div>
                  </div>
                </div>
                <div class="inline__item">
                  <div class="indicator">
                    <div class="indicator__value indicator__value_small">4.5B</div>
                    <div class="indicator__name indicator__name_light">Events</div>
                  </div>
                </div>
                <div class="inline__item">
                  <div class="indicator">
                    <div class="indicator__value indicator__value_small">32%</div>
                    <div class="indicator__name indicator__name_light">Retention</div>
