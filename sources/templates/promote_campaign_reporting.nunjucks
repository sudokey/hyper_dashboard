{% set activeSection = 'promote' %}
{% set activePage = 'campaign_reporting' %}

{% extends "layout.nunjucks" %}

{% block content %}
  <div class="content">
    <div class="content__col content__col_side">
      {% include "sidebar_promote.nunjucks" %}
    </div>

    <div class="content__col content__col_main">
      <div class="content__title">
        <div class="pan">
          <div class="pan__section pan__section_left">
            <h1 class="title">Campaign Reporting</h1>
          </div>

          <div class="pan__section pan__section_right">
            <a href="promote_create_campaing.html" class="button button_green">
              <span class="icon icon_plus icon_white"></span>
              <span class="button__text">Create Campaing</span>
            </a>
          </div>
        </div>
      </div>

      <div class="content__section">
        <div class="table-panel">
          <div class="table-panel__header">
            <div class="pan">
              <div class="pan__section pan__section_left">
                <div class="inline inline_size_md">
                  <div class="inline__item">
                    {% include "popover_filters.nunjucks" %}
                  </div>
                  <div class="inline__item">
                    <div class="field">
                      <div class="select-small-wrapper">
                        <select class="js-select" placeholder="Select Campaign" style="width: 18rem">
                          <option value="" disabled selected>Select Campaign</option>
                          <option value="1">Test 1</option>
                          <option value="2">Test 2</option>
                        </select>
                      </div>
                    </div>
                  </div>
                  <div class="inline__item">
                    <div class="field">
                      <div class="select-small-wrapper">
                        <select class="js-select" placeholder="Group by" style="width: 12rem">
                          <option value="" disabled selected>Group by</option>
                          <option value="1">Test 1</option>
                          <option value="2">Test 2</option>
                        </select>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <div class="pan__section pan__section_right">
                <div class="inline inline_size_md">
                  <div class="inline__item">
                    <a class="js-daterange" href="javascript:void(0)"></a>
                  </div>
                  <div class="inline__item">
                    <a href="#" class="button button_bordered_blue button_small">
                      <span class="button__icon">
                        <span class="icon icon_export icon_white"></span>
                        <span class="icon icon_export icon_blue"></span>
                      </span>
                      Export
                    </a>
                  </div>
                  <div class="inline__item">
                    <a href="#" class="button button_blue button_small"><strong>Run Report</strong></a>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="table-wrapper">
            <div class="table-wrapper__settings">
              <div class="popover js-popover">
                <div class="popover__toggler js-popover-toggle">
                  <span class="icon icon_settings"></span>
                </div>

                <div class="popover__content popover__content_bottom-right">
                  <div class="panel panel_popover panel_settings">
                    <label class="checkbox">
                      <input type="checkbox">
                      <span class="checkbox__content">
                        <span class="checkbox__ctrl"></span>
                        <span class="checkbox__label">Ad Name</span>
                      </span>
                    </label>

                    <label class="checkbox">
                      <input type="checkbox">
                      <span class="checkbox__content">
                        <span class="checkbox__ctrl"></span>
